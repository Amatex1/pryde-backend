{
  "name": "pryde-social-client",
  "private": true,
  "version": "1.0.0-rc.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "optimize-images": "node scripts/optimize-images.js",
    "test": "mocha --require server/test/setup.js server/test/**/*.test.js --timeout 10000",
    "test:watch": "mocha --require server/test/setup.js server/test/**/*.test.js --timeout 10000 --watch",
    "audit": "node server/scripts/runFullAudit.js",
    "audit:json": "node server/scripts/runFullAudit.js > audit-report.json",
    "audit:dry-run": "node server/scripts/runFullAuditDryRun.js",
    "audit:pryde": "node server/scripts/prydeAudit.js",
    "audit:theme": "node server/scripts/themeLeakAudit.js",
    "audit:paths": "node server/scripts/userImpactAudit.js",
    "audit:health": "node server/scripts/codeHealthAudit.js",
    "audit:final": "node server/scripts/prydeAudit.js && node server/scripts/themeLeakAudit.js && node server/scripts/userImpactAudit.js && node server/scripts/codeHealthAudit.js && node server/scripts/generateAuditReport.js",
    "audit:auth": "node server/scripts/runtimeAuthAudit.js",
    "release:check": "node server/scripts/releaseGuard.js",
    "freeze:check": "node server/scripts/freezeGuard.js",
    "polish:theme": "node server/scripts/themeLeakFixer.js",
    "polish:theme:dry": "node server/scripts/themeLeakFixer.js --dry-run",
    "test:mongo": "node server/scripts/testMongoConnection.js",
    "fix:mongo": "node server/scripts/fixMongoConnectionString.js",
    "backup": "node server/scripts/backupToCloud.js",
    "backup:all": "node server/scripts/backupAll.js"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^1.17.1",
    "@simplewebauthn/browser": "^13.2.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dompurify": "^3.3.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "focus-trap-react": "^11.0.4",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.1",
    "isomorphic-dompurify": "^2.34.0",
    "nodemailer": "^7.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "redis": "^5.10.0",
    "socket.io-client": "^4.7.2",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@humanwhocodes/config-array": "^0.13.0",
    "@humanwhocodes/object-schema": "^2.0.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@ungap/structured-clone": "^1.3.0",
    "@vitejs/plugin-react": "^4.0.3",
    "ansi-regex": "^5.0.1",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "doctrine": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3",
    "espree": "^9.6.1",
    "file-entry-cache": "^6.0.1",
    "flat-cache": "^3.2.0",
    "fs.realpath": "^1.0.0",
    "glob": "^7.2.3",
    "globals": "^13.24.0",
    "graphemer": "^1.4.0",
    "inflight": "^1.0.6",
    "mocha": "^11.7.5",
    "path-is-absolute": "^1.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "strip-ansi": "^6.0.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "text-table": "^0.2.0",
    "type-fest": "^0.20.2",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "web-vitals": "^5.1.0",
    "workbox-window": "^7.4.0"
  }
}
